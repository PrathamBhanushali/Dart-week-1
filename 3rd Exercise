void main(){
  
  //1. sum of the items in the list
  
  List<int>integers=[1,3,5,7,9];
  
  int sum=0;
  for(int i=0;i<integers.length;i++){
    sum=sum+integers[i];
  }
  print(sum);
  
  //2. sets
  
  const a =<int>{1,3};
  const b =<int>{3,5};
  var c = a.difference(b);
  var d = b.difference(a);
  print(c.union(d));
  
}
